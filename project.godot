[autoload]

Settings="*res://Settings/dist/Settings.gd"
"数据"="*res://脚本/数据/数据.gd"
"弹窗"="res://资源/场景/UI/弹窗.tscn"
changer_scence="*res://资源/场景/UI/场景过渡.tscn"
Uhd="*res://资源/场景/UI/UHD.tscn"
GameState="*res://game_state.gd"
mmkv="*res://addons/godot-mmkv/mmkv.gd"
